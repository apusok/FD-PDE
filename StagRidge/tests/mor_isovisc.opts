# model options
-tests 1
-dim 0 # 0-dimensionless 1-dimensional 
-mtype 1 # model type 0-solcx 1-mor_analytic
-nx 20
-nz 20
-xmin 0.0 # start domain x-dir [km]
-zmin -1.0 # start domain z-dir [km]
-L 1.0 # length domain [km]
-H 1.0 # height domain [km]
-g 1.0 # gravitational acceleration [m/s2]
-u0 1 # half-spreading rate [cm/yr]
-rangle 0 # ridge angle [deg]
-eta0 1.0 # reference viscosity [Pa.s]
-rho0 0.0 # reference density [kg/m3]
-output_file mor_isovisc

# solver options
-pc_type lu
-pc_factor_mat_solver_type umfpack
#-snes_mf_operator
